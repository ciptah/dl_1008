{
  "verbose": true,
  "unlabeled_model": "vae",
  "skip_unlabeled_training": true,
  "skip_labeled_training": false,
  "augment_training_data": "vae",

  "training": {
    "learning_rate": 0.1,
    "dropout": 0.4,
    "num_unlabeled_epochs": 7,
    "num_epochs": 100,
    "nonlinearity": "relu",
    "momentum": {
      "mu_coefficient": 0.05,
      "use_nesterov": false
    }
  },

  "vae": {
    "num_latent_vars": 16,
    "image_per": 2,
    "continue": false,
    "kl_multiplier": 1,
    "score_fn": "squared_error",
    "image_file_name": "vae_squared.png",
    "model_file_name": "vae_squared.torch.tar"
  },

  "vae_expander": {
    "p": 0.9,
    "model_file_name": "vae_squared.torch.tar"
  }
}
