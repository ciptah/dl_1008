{
  "verbose": true,
  "unlabeled_model": "vae",
  "skip_unlabeled_training": false,
  "skip_labeled_training": true,

  "training": {
    "learning_rate": 3e-4,
    "num_unlabeled_epochs": 8,
    "nonlinearity": "relu"
  },

  "vae": {
    "num_latent_vars": 16,
    "image_per": 1,
    "continue": false,
    "kl_multiplier": 1,
    "score_fn": "squared_error",
    "image_file_name": "vae_squared_e8.png",
    "model_file_name": "vae_squared_e8.torch.p"
  }
}
