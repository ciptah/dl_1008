{
  "verbose": true,
  "unlabeled_model": "vae",
  "skip_labeled_training": true,

  "training": {
    "learning_rate": 8e-5,
    "num_unlabeled_epochs": 3,
    "nonlinearity": "relu"
  },

  "vae": {
    "num_latent_vars": 2,
    "image_per": 1,
    "continue": true,
    "use_convnets": true,
    "fc_limit": 1,
    "kl_multiplier": 1
  }
}
