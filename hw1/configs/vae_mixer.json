{
  "verbose": true,
  "model": "improved",
  "skip_unlabeled_training": true,
  "skip_labeled_training": false,
  "augment_training_data": "chain",

  "training": {
    "learning_rate": 0.1,
    "dropout": 0.4,
    "num_epochs": 180,
    "nonlinearity": "relu",
    "epochs_before_unlabeled": 100000,
    "momentum": {
      "mu_coefficient": 0.05,
      "use_nesterov": false
    }
  },

  "vae_expander": {
    "p": 0.5,
    "mode": "hull",
    "hull_dim": 4,
    "extra": 0.2,
    "epoch_multiple": 1,
    "model_file_name": ["vae_squared_e8.torch.p"]
  }
}

