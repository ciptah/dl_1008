{
  "verbose": true,
  "template": {
    "data": "./data/penn",
    "model": "LSTM",
    "emsize": 50,
    "nhid": 50,
    "nlayers": 1,
    "lr": [0.001, 0.0001, 0.00001],
    "momentum": 0.05,
    "weight_decay": 0.01,
    "clip": 0.5,
    "epochs": 20,
    "batch_size": 20,
    "sequence_length": 20,
    "seed": 1111,
    "log_interval": 20,
    "logfile": "logs/{}.txt",
    "results": "results/{}.txt",
    "save": "models/{}.pt"
  },
  "max_saved_models": 50,
  "training_cap": 100
}
