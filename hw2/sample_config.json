{
  "verbose": true,
  "template": {
    "data": "./data/penn",
    "model": "LSTM",
    "optim": "adam",
    "emsize": 50,
    "nhid": [50, 100],
    "nlayers": 1,
    "lr": 20,
    "weight_decay": 0,
    "clip": 0.5,
    "epochs": 3,
    "batch_size": 20,
    "vocab_size": 10000,
    "sequence_length": 20,
    "seed": 1111,
    "log_interval": 200,
    "logfile": "logs/{}.txt",
    "results": "results/{}.txt",
    "save": "models/{}.pt"
  },
  "max_saved_models": 50,
  "training_cap": 100
}
