{
  "verbose": true,
  "template": {
    "data": "./data/penn",
    "model": ["LSTM"],
    "emsize": [50],
    "nhid": [50],
    "nlayers": [1],
    "lr": 20,
    "clip": [0.5],
    "epochs": 6,
    "batch_size": 20,
    "sequence_length": [20],
    "seed": 1111,
    "log_interval": 20,
    "logfile": "logs/{}.txt",
    "results": "results/{}.txt",
    "save": "models/{}.pt",
    "shuffle": true,
    "initialization": {
      "word_embedding": "glove",
      "weights": "zero",
      "hidden_state": "zero"
    },
    "len_relation":false
  },
  "max_saved_models": 50,
  "training_cap": 100
}
