{
  "verbose": true,
  "template": {
    "data": "./data/penn",
    "model": "LSTM",
    "emsize": 50,
    "nhid": 50,
    "nlayers": [2, 3],
    "lr": [20, 10],
    "momentum": [0.3, 0.2, 0.1],
    "weight_decay": 0,
    "clip": 0.5,
    "epochs": 20,
    "batch_size": 20,
    "sequence_length": 20,
    "seed": 1111,
    "log_interval": 20,
    "logfile": "logs/{}.txt",
    "results": "results/{}.txt",
    "save": "models/{}.pt"
  },
  "max_saved_models": 50,
  "training_cap": 100
}
